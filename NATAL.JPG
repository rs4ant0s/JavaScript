<script>
	function readCookieClimba(cname) {
		var name = cname + "=";
		var decodedCookie = decodeURIComponent(document.cookie);
		var ca = decodedCookie.split(';');
		for (var i = 0; i < ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0) == ' ') {
				c = c.substring(1);
			}
			if (c.indexOf(name) == 0) {
				return c.substring(name.length, c.length);
			}
		}
		return false;
	}

	function closeBarraPromocao(){

		document.querySelector('.barra-promocional-top').style.display  = "none";
		document.querySelector('.barra-promocional-top').style.removeProperty('padding-top');
		writeCookieClimba('climba_barra_promocional_closed', '1', 1);

		const mainContentElements = document.querySelectorAll('.main-content');
		mainContentElements[0].classList.remove('barra-promocional');

		const topoLojaElements = document.querySelectorAll('.topo-loja');
		topoLojaElements[0].classList.remove('barra-promocional');

	}

	if(window.matchMedia( "(max-width: 1000px)").matches) {
		const topoLoja = document.querySelector('.topo-loja');
		const contentTopoFixoMobile = document.querySelector('#content.topo-fixo-mobile');
		const contentBarraPromocional = document.querySelector('.content-barra-promocional');

		topoLoja.classList.add('barra-promocional');

		const topoLojaHeight = topoLoja.clientHeight;
		const contentBarraPromocionalHeight = contentBarraPromocional.clientHeight;

		contentTopoFixoMobile.style.paddingTop = topoLojaHeight + contentBarraPromocionalHeight + 'px';
	}

	if(!readCookieClimba('climba_barra_promocional_closed')) {
		document.querySelector('.barra-promocional-top').style.display  = "block";
	} else {
		const mainContentElements = document.querySelectorAll('.main-content');
		mainContentElements[0].classList.remove('barra-promocional');
	}
</script>
