<script type="text/javascript">
	function closePopupWithFadeOut() {
		$(".fundo-popup").fadeOut(400, function(){
			closePopup();
		});
	}

	function closePopup() {
		$('.delay-fundo-popup').fadeOut();
	}

	function openPopup() {
				const img = document.getElementById('image-popup');
		$('.fundo-popup').fadeIn();

		const widthRendered = img.clientWidth;
		const heightRendered = img.clientHeight;
		$('.popup-buttons').css('max-width', widthRendered + 'px').css('max-height', heightRendered + 'px');
			}

	function handleShowPopup() {
		if (window.matchMedia("(max-width: 1000px)").matches) {
			closePopup();
		} else {
			renderizePopUp();
		}
	}

		$('#image-popup').on('load', function(){
		renderizePopUp();
	})

	$(window).resize(function() {
		handleShowPopup();
	});
	
	
	function renderizePopUp() {
		if(!readCookieClimba('climba_popup_open')) {
			if (window.matchMedia("(min-width: 1001px)").matches) {
				openPopup();
			}

			writeCookieClimba('climba_popup_open', '1', 1);
		}
	}

